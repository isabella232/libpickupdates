{
  "name": "npm-update-interactive-component",
  "version": "1.0.0",
  "author": {
    "name": "Kat March√°n",
    "email": "kzm@zkat.tech",
    "twitter": "maybekatz"
  },
  "license": "ISC",
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "watch": "babel -w src --out-dir dist",
    "prerelease": "npm t",
    "release": "standard-version -s",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "standard",
    "test": "tap -J --100 test/*.js",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "standard": "*",
    "standard-version": "*",
    "tap": "*",
    "weallbehave": "*",
    "weallcontribute": "*"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.0.0"
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm-update-interactive-component.git"
  },
  "bugs": "https://github.com/npm/npm-update-interactive-component/issues",
  "homepage": "https://npmjs.com/package/npm-update-interactive-component",
  "dependencies": {
    "ink": "^2.0.0-12",
    "libnpm": "^2.0.1",
    "react": "^16.8.2",
    "semver": "^5.6.0"
  },
  "resolutions": {
    "react-reconciler": "0.20.0",
    "scheduler": "0.13.2"
  }
}
